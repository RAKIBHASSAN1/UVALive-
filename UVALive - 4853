#include<iostream>
#include<cstdio>
using namespace std;
int main()
{
int i,j,c,k,v,z=0;
while(cin>>i)
{
    if(i==0)
    {
        break;
    }
    else
    {
        z++;
    c=0;
    k=0;
    for(j=0;j<i;j++)
    {
        int a[i];
        cin>>a[j];
        if(a[j]==0)
        {

            c++;
        }
        else

            {
            k++;

            }
    }
    v=k-c;
    cout<<"Case "<<z<<": "<<v<<endl;
}

    }

}
